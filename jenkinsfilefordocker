node
{
stage('scm checkout'){
git 'https://github.com/devinameena/maven-project'
}
stage('package stage'){
sh label: '', script: 'mvn clean package'
}
stage ('Checkout to different branch') {
sh "git branch -r"
sh "git checkout master"
}
stage('docker image build'){
sh 'docker build -t devina0110/tom-app:1.0.0 .'
}
stage('push image to docker hub'){
withCredentials([usernameColonPassword(credentialsId: 'dockerhubid', variable: '')]) {
   sh "docker login -u devina0110 -p ${dockerhubid}"
  }
  sh 'dokcer push devina0110/tom-app:1.0.0'
}
}
